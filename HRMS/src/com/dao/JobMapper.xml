<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dao.JobMapper">
    <insert id="addjob">
        insert INTO JOB VALUES (JOB_SEQ.nextval,#{dempartment.id},#{name},#{createtime})
    </insert>
    <update id="updatejob">
        UPDATE JOB SET name=#{name} where J_ID=#{id}
    </update>
    <delete id="deletejob">
        DELETE form JOB where J_ID=#{id}
    </delete>
    <select id="Alljob" resultType="com.model.Job">
        SELECT * from JOB
    </select>
</mapper>